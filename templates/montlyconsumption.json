[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-licensemonth-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "operation": "monthlyconsumption-list",
        "side_effects": false,
        "system": "{{@ system @}}",
        "trace": true,
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "copy",
              "*",
              "response"
            ],
            [
              "create-child",
              [
                "apply",
                "lines",
                "_S.response.lines"
              ]
            ]
          ],
          "lines": [
            [
              "merge",
              [
                "map",
                [
                  "dict",
                  [
                    "nth",
                    "_.",
                    "_R._S.response.headers"
                  ],
                  [
                    "nth",
                    "_.",
                    "_S."
                  ]
                ],
                [
                  "range",
                  0,
                  [
                    "count",
                    "_R._S.response.headers"
                  ]
                ]
              ]
            ],
            [
              "add",
              "license_id",
              "_R._S.license_id"
            ],
            [
              "add",
              "period",
              "_R._S.date_start"
            ],
            [
              "add",
              "_id",
              [
                "concat",
                "_R._S.license_id",
                "_",
                "_T.Resource Group",
                "_",
                "_T.Resource Name",
                "_",
                "_R._S.date_start",
                "_",
                "_T.Vendor Ressource SKU",
                "_T.Region"
              ]
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all2",
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "type": "dataset"
    },
    "transform": {
      "type": "emit_children"
    },
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all2",
      "type": "dataset"
    },
    "type": "pipe"
  }
]